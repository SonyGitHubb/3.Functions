    "def arithmetic_average(lst = []):\n",
    "    if len(lst) != 0:\n",
    "        return sum(lst) / len(lst)\n",
    "    else:\n",
    "        return 0\n",
    "\n",
    "result = arithmetic_average([1, 2, 3, 4, 5])\n",
    "print(result)\n"
   ]
